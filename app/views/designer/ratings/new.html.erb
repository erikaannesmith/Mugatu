<div class='new-app'>
  <h1>Please take a moment to rate your order!</h1>

  <%= form_for [:designer, @rating.order, @rating] do |f| %>

    <%= f.label :efficiency %>
    <%= f.select :efficiency, [1, 2, 3, 4, 5], class: 'form-control' %>

    <%= f.label :accuracy %>
    <%= f.select :accuracy, [1, 2, 3, 4, 5], class: 'form-control' %>

    <%= f.submit "Rate Order", class: 'btn btn-primary new-btn' %>

  <% end %>
</div>
    