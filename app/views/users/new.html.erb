<div class="sign-up-field">
  <%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :role %>
    <%= f.select :role, ["designer", "producer"] %>

    <%= f.submit "Sign Up" %>
  <% end %>
</div>